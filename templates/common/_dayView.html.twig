{% extends '@EasyAdmin/page/content.html.twig' %}
{% block page_content %}
	<h1>
		{% trans %}
		dayView
		{% endtrans %}
	</h1>

	<table class="table table-condensed">
		<thead>
			<tr>
				<th>Time</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			{% for i in 0..20 %}
				<tr>
					<td>
						{{toTime(420+ i * 30)}}-{{toTime(420+ (i+1) * 30)}}
					</td>
					<td></td>
				</tr>
			{% endfor %}

		</tbody>

	</table>

{% endblock %}
